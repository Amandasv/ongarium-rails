<div class ="container">
	<div class="col-lg-12 text-center">
		<h1>Nova ONG</h1>
	</div>
	<%= render 'form'%>
</div>
<div id="address">
	<%= render './addresses/address_form', f:f %>
</div>
<div class="row">
	<div class="col-lg-offset-2 col-lg-10 form-group">
		<%= f.check_box :privacy, :id =>"privacy-ngo" %>
		<%= f.label "Ocultar dados da ONG!",:for => "privacy-ngo" %>
	</div>
</div>
<%= render 'users/form', f:f %>
<div class="row">
	<div class="col-lg-offset-4 col-lg-4 margin-end-top margin-end-bot">
		<%= f.submit "Cadastrar ONG", :class => "btn btn-default btn-lg btn-block btn-primary" %>
	</div>
</div>
<% end %>
</div>