<%= f.fields_for :user do |ff| %> <!-- Model Address-->
	<div class="form-group">
		<label class="control-label col-lg-3">
			<h3>Usuário:</h3>
		</label>
		<div class="col-lg-5">
			
		</div>
	</div>
	<div class="form-group">
		<label class="control-label col-lg-3" for="email-user">
			Email:
		</label>
		<div class="col-lg-5">
			<%= ff.text_field :email, :id => "email-user", :placeholder => 'exemplo@exemplo.com', :class => "form-control validate-email" %>
		</div>
    <div id="emailErrorInvalid" class="col-lg-2 alert alert-danger custom-alert-danger email-error-invalid">
    <p>Email Inválido!</p>
  </div>
    <div id="emailErrorNil" class="col-lg-2 alert alert-danger custom-alert-danger empty-error-email-user">
      <p>Campo Obrigatório!</p>
    </div>
	</div>
	
	<div class="form-group">
		<label class="control-label col-lg-3" for="password-user">
			Senha:
		</label>
		<div class="col-lg-5">
			<%= ff.password_field :password, :id => "password-user", :class => "form-control validate-password" %>
		</div>
		<div id="passwordError" class="col-lg-2 alert alert-danger custom-alert-danger empty-error-password-user">
			<p>Campo Obrigatório!</p>
		</div>
	</div>
	<div class="form-group">
		<label class="control-label col-lg-3" for="password-confirmation-user">
			Confirmar Senha:
		</label>
		<div class="col-lg-5">
			<%= ff.password_field :password_confirmation, :id => "password-confirmation-user", :class => "form-control validate-password-confirmation-user" %>
		</div>

		<div id="passwordConfirmationError" class="col-lg-2 alert alert-danger custom-alert-danger password-confirmation-invalid">
			<p>Senhas não são iguais!</p>
		</div>
    <div id="passwordConfirmationErrorNil" class="col-lg-2 alert alert-danger custom-alert-danger empty-error-password-confirmation-user">
      <p>Campo Obrigatório!</p>
    </div>

	</div>
<% end %>