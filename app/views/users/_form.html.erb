<%= f.fields_for :user do |ff| %> <!-- Model Address-->
	<div class="row">
		<div class="col-lg-offset-2 col-lg-3">
		<label class="control-label", for="email-user">Email: <span class="mandatory">*</span></label>
		</div>
	</div>
	<div class="row">
		<div class="col-lg-offset-2 col-lg-3 form-group">
			<%= ff.text_field :email, :id => "email-user", :placeholder => 'exemplo@exemplo.com', :class => "form-control validate-email" %>
		</div>
		<div class="col-lg-5">
			<div id="emailErrorInvalid" class="col-lg-5 alert alert-danger custom-alert-danger email-error-invalid">
	    		<p>Email Inválido!</p>
	  		</div>
  		</div>
	</div>
	
	<div class="row">
		<div class="col-lg-offset-2 col-lg-3">
		<label class="control-label", for="password-user">Senha: <span class="mandatory">*</span></label>
		</div>
	</div>
	<div class="row">
		<div class="col-lg-offset-2 col-lg-3 form-group">
			<%= ff.password_field :password, :id => "password-user", :class => "form-control validate-password" %>
		</div>
		
	</div>
	<div class="row">
		<div class="col-lg-offset-2 col-lg-3">
			<label class="control-label", for="password-confirmation-user">Confirmar senha: <span class="mandatory">*</span></label>
		</div>
	</div>
	<div class="row">
		<div class="col-lg-offset-2 col-lg-3 form-group">
			<%= ff.password_field :password_confirmation, :id => "password-confirmation-user", :class => "form-control validate-password-confirmation-user" %>
		</div>
	<div>
		<div id="passwordConfirmationError" class="col-lg-4 alert alert-danger custom-alert-danger password-confirmation-invalid">
			<p>Senhas não são iguais!</p>
		</div>
	</div>
	</div>
<% end %>